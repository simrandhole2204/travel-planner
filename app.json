{
  "expo": {
    "name": "Travel Planner",
    "slug": "travel-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.travelplanner.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby places and help plan your trips.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show nearby places and help plan your trips."
      }
    },
    "android": {
      "package": "com.travelplanner.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM"
      ],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker",
      "expo-asset",
      "expo-font"
    ],
    "scheme": "travelplanner",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c3a98942-46de-4b4c-8d0a-a4d7ca65dc3b"
      }
    }
  }
}